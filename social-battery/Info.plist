<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>en</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleName</key>
    <string>social-battery</string>
    <key>NSCalendarsUsageDescription</key>
    <string>This app reads your calendars to detect social events and improve friend scheduling suggestions while running locally.</string>
    <key>NSRemindersUsageDescription</key>
    <string>This app may access reminders to surface planned social activities.</string>
</dict>
</plist>
